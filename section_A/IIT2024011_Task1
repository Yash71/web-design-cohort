document.querySelector("button").addEventListener("click", ()=>{
    document.querySelector("div").innerHTML="This is a 'div' element filled using javascript.";
    document.querySelector("p").style.margin="20px 0px;";
    document.querySelector("p").innerHTML="This is a 'p' element filled using javascript.";
    document.querySelector("a").innerHTML+=`This is an 'a' element filled using javascript.`;
});
